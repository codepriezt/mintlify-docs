{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Cloudsania",
  "logo": {
    "dark": "/logo/dark.png",
    "light": "/logo/light.png"
  },
  "favicon": "/cloudsania-icon.png",
  "colors": {
    "primary": "#7C4FFF",
    "light": "#D9A15F",
    "dark": "#DE5136",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "https://docs.cloudsania.com/help-and-support/contact-support-and-community",
      "type": "link"
    },
    {
      "name": "Web Console",
      "url": "https://console.cloudsania.com/auth/sign-in",
      "type": "link"
    }
  ],
  "topbarCtaButton": {
    "name": "Start 30-Day Trial",
    "url": "https://console.cloudsania.com/auth/sign-up"
  },
  "search": {
    "prompt": "Search documentation...",
    "placeholder": "Search...",
    "containerId": "docsearch",
    "buttonText": "Search",
    "maxWidth": "400px"
  },
  "layout": "sidenav",
  "css": ["css/styles.css"],
  "modeToggle": {
    "default": "light",
    "isHidden": false
  },
  "feedback": {
    "suggestEdit": true,
    "raiseIssue": true,
    "rating": true
  },
  "templates": {
    "default": "_template.mdx"
  },
  "navigation": [
    {
      "group": "Home",
      "pages": ["overview"]
    },
    {
      "group": "Introduction",
      "pages": [
        "introduction/about-cloudsania",
        "introduction/how-cloudsania-works",
        "introduction/basic-concepts"
      ]
    },
    {
      "group": "Getting Started",
      "pages": [
        "getting-started/account-setup",
        "getting-started/quick-start"
      ]
    },
    {
      "group": "Cloud Connectors",
      "pages": [
        "cloud-connectors/cloud-providers",
        "cloud-connectors/cloud-connectors",
        "cloud-connectors/aws-connector",
        "cloud-connectors/vps-connector"
      ]
    },
    {
      "group": "Project Management",
      "pages": [
        "projects/project-in-cloudsania",
        "projects/app-service"
      ]
    },
    {
      "group": "Environment Setup",
      "pages": [
        "environments/environments",
        "environments/konstacks",
        "environments/addons",
        "environments/dns-configurations"
      ]
    },
    {
      "group": "Integrations",
      "pages": [
        "integrations/github",
        "integrations/cloud-flare",
        "integrations/digital-ocean",
        "integrations/route53"
      ]
    },
    {
      "group": "Deployment",
      "pages": [
        "deployments/cicd-pipelines"
      
      ]
    },
    {
      "group": "How to Guides",
      "pages": [
        "how-to-guides/how-to-setup-aws-connector",
        "how-to-guides/how-to-setup-vps-connector",
        "how-to-guides/how-to-create-a-project",
        "how-to-guides/how-to-create-an-environment",
        "how-to-guides/how-to-deploy-an-appservice",
        "how-to-guides/how-to-deploy-a-konstack",
        "how-to-guides/how-to-deploy-addons",
        "how-to-guides/how-to-integrate-github",
        "how-to-guides/how-to-integrate-cloudflare",
        "how-to-guides/how-to-integrate-digitalOcean",
        "how-to-guides/how-to-integrate-route53",
        "how-to-guides/how-to-configure-DNS-records",
        "how-to-guides/how-to-setup-a-cicd-pipeline",
        "how-to-guides/how-to-schedule-a-call"
      ]
    },
    {
      "group": "Account Settings",
      "pages": [
        "account-settings/ssh-key-setup",
        "account-settings/team-management"
      ]
    },
    {
      "group": "Help and Support",
      "pages": [
        "help-and-support/schedule-a-call",
        "help-and-support/contact-support-and-community",
        "help-and-support/faqs"
      ]
    }
  ],
  "footerSocials": [
    {
      "type": "website",
      "url": "https://blog.cloudsania.com/"
    },
    {
      "type": "x-twitter",
      "url": "https://x.com/cloudsania"
    },
    {
      "type": "instagram",
